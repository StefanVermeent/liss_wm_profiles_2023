	author = {Ugarte, Elisa and Hastings, Paul D.},
	month = apr,
	year = {2023},
	keywords = {unpredictability, caregiver unpredictability, dynamic systems, entropy, environmental statistics},
	pages = {1--20},
	file = {Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\ESHKHB7R\\Ugarte and Hastings - 2023 - Assessing unpredictability in caregiver–child rela.pdf:application/pdf},
}

