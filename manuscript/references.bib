	keywords = {Working memory, Short term memory},
	pages = {1158--1170},
	file = {Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\9X8MTYXV\\Cowan - 2017 - The many faces of working memory and short-term st.pdf:application/pdf},
}

